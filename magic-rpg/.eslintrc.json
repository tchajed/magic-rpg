{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "always-multiline"],
        "no-unused-vars": "error",
        "no-const-assign": "error",
        "object-shorthand": "error",
        "valid-typeof": "error",
        "eqeqeq": "error",
        "prefer-const": ["error", {"destructuring": "all"}],
        "no-useless-escape": "error",
        "quotes": ["error", "single"]
    }
}
